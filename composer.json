{
    "name": "mchev/meteofrance-php",
    "description": "This is my package meteofrance-php",
    "keywords": [
        "mchev",
        "meteofrance-php"
    ],
    "homepage": "https://github.com/mchev/meteofrance-php",
    "license": "MIT",
    "authors": [
        {
            "name": "mchev",
            "email": "martin.chevignard@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Mchev\\MeteofrancePhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mchev\\MeteofrancePhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
